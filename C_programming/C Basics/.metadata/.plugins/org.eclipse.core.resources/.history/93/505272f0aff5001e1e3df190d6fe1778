#include <stdio.h> // calling stander input output lib
void main (){
	float n1 , n2 ;
	printf("enter two numbers : ");
	fflush(stdin);fflush(stdout); //clearing the buffer
	scanf("%d %d",&n1,&n2);
	printf("the sum is = %d",(n1+n2));

}
